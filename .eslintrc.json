{
  "root": true,
  "ignorePatterns": ["**/*"],
  "plugins": ["@nx"],
  // My custom config
  "extends": ["gearonix"],

  "rules": {
    "react/react-in-jsx-scope": "off",
    "quotes": [2, "single", { "avoidEscape": true }],
    "@typescript-eslint/no-namespace": "off",
    "@typescript-eslint/member-delimiter-style": ["error",  {"multiline" : {"delimiter": "none"}}]
  }
}
